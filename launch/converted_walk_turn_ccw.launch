<launch>
  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value="" />
  <arg     if="$(arg debug)" name="launch_prefix" value="xterm -e gdb --args python" />

  <node launch-prefix="$(arg launch_prefix)" pkg="valkyrie_testing_edi" type="converted_footstep.py" name="ValkyrieWalkingTestNode" output="screen">
    <param name="DataFile" type="string" value="$(find valkyrie_testing_edi)/data/converted_walk_turn_ccw.yaml" />
  </node>

</launch>
